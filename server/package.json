{
  "name": "dynamodb-dashboard",
  "version": "1.9.3",
  "description": "GUI Dashboard for local or remote DynamoDB",
  "main": "index.js",
  "bin": {
    "dynamodb-dashboard": "./build/bin/cli.js"
  },
  "scripts": {
    "start": "node build",
    "dev": "nodemon --exec npm run babel-node -- src",
    "build": "rimraf build && babel src --out-dir build",
    "babel-node": "babel-node --presets='@babel/preset-env'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettify": "prettier --write 'src/**/*.{js,ts}'",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix"
  },
  "author": "Kritish Dhaubanjar",
  "keywords": [
    "UI",
    "Local",
    "Amazon",
    "DynamoDB"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/node": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.25.2",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.154.0",
    "@aws-sdk/credential-providers": "^3.171.0",
    "@aws-sdk/lib-dynamodb": "^3.154.0",
    "commander": "^11.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "morgan": "^1.10.0",
    "open": "^10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kritish-dhaubanjar/dynamodb-dashboard.git"
  }
}
